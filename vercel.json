{
    "version": 2,
    "buildCommand": "npm run build",
    "startCommand": "npm run start:prod",
    "outputDirectory": "dist",
    "builds": [
        {
            "src": "dist/main.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/dist/main.js"
        }
    ]
}